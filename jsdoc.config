{
    "tags": {
      "allowUnknownTags": true,
      "recurseDepth": 10,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["./src"],
      "includePattern": ".+\\.js$"
    },
    "opts": {
      "destination": "public/docs",
      "recurse": true,
      "readme": "README.md",
      "template": "node_modules/clean-jsdoc-theme",
      "theme_opts": {
          "default_theme": "dark"
      }
    },
    "sourceType": "module",
    "plugins": ["plugins/markdown"],
    "templates": {
      "default": {
        "includeDate": false
      }
    }
}